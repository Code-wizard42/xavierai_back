# Fast Development Configuration
# Use this for faster startup: python run_app.py --config=fast

FLASK_ENV=development
FAST_MODE=true

# Disable heavy components
DISABLE_NLTK=true
DISABLE_FIREBASE=true
DISABLE_VECTOR_DB=true
DISABLE_SCHEDULER=true
DISABLE_REDIS=true
SKIP_ENV_CHECKS=true

# Use local/fast alternatives
USE_FALLBACK_EMBEDDINGS=true
USE_LOCAL_LLM=true
SESSION_TYPE=filesystem

# Minimal logging
MINIMAL_LOGGING=true 